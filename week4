## first question

f = open('english.txt', 'r')

txt = f.read().split(', ')
list =[]

dictionary = {}
for x in txt:
  x = x.strip()
  list.append(x)

list1 = list.split('()')
list2 = []
list3 = []
for y in list1:
 if (y%2 ==1):
  list3.append(list1[y])
 else:
  list2.append(list1[y])
  
final = zip(list2,list3)

final1 = dict(final)

print(final1)
















