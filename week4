## first question


import re
f = open('english.txt', 'r')
text = f.read()
text= text.strip()
txt = re.split(',| ()', text)

list1 =[]
list2 = []
dictionary = {}
n = len(txt)
for i in range (n):
  if (i%2 == 1):
   list2.append(txt[i])
  else:
    list1.append(txt[i])
final = zip(list1,list2)
final1 = dict(final)

print(final1)















